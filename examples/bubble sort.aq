#name bubble sort
#use functions.txt

decl l [4, 1, 6, -3, 2]
decl i 0
decl int j
decl len length($l)

trace $l

for ($i = 0, $i < $len, $i = $i + 1)
	for ($j = 0, $j < $len - $i - 1, $j = $j + 1)
		if (list_at($l, $j) > list_at($l, $j + 1))
			swap($l, $j, $j + 1)
		end-if
	end-for
end-for

return($l)
